generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Food {
  id      Int    @id @default(autoincrement())
  name    String @unique
  protein Float
  fat     Float
  carb    Float
  kcal    Float
  fiber   Float
}
